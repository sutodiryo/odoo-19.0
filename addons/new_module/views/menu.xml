<odoo>
    <!-- Actions -->
    <record id="action_real_estate_property" model="ir.actions.act_window">
        <field name="name">Properties</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_real_estate_agent" model="ir.actions.act_window">
        <field name="name">Agents</field>
        <field name="res_model">real.estate.agent</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Menus -->
    <menuitem id="menu_real_estate_root" name="Real Estate" sequence="10"/>
    <menuitem id="menu_real_estate_property" name="Properties" parent="menu_real_estate_root" action="action_real_estate_property" sequence="20"/>
    <menuitem id="menu_real_estate_agent" name="Agents" parent="menu_real_estate_root" action="action_real_estate_agent" sequence="30"/>
</odoo>
