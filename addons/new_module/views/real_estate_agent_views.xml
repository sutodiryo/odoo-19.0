<odoo>
    <record id="view_real_estate_agent_list" model="ir.ui.view">
        <field name="name">real.estate.agent.list</field>
        <field name="model">real.estate.agent</field>
        <field name="arch" type="xml">
            <list string="Agents">
                <field name="name"/>
                <field name="phone"/>
                <field name="email"/>
            </list>
        </field>
    </record>

    <record id="view_real_estate_agent_form" model="ir.ui.view">
        <field name="name">real.estate.agent.form</field>
        <field name="model">real.estate.agent</field>
        <field name="arch" type="xml">
            <form string="Agent">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="phone"/>
                        <field name="email"/>
                        <field name="property_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
